<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>12.0</LangVersion>
    <AssemblyTitle>H.264 ContentDotNet extension</AssemblyTitle>
    <Description>Adds H.264 parsing, editing and exporting support for ContentDotNet</Description>
    <Company>winscripter</Company>
    <Copyright>Copyright (c) 2023-2025, winscripter</Copyright>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);SUPPRESS_CHECKS_THAT_COULD_SLOW_THINGS_DOWN</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);SUPPRESS_CHECKS_THAT_COULD_SLOW_THINGS_DOWN</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Containers\Container32.gen.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Container32.tt</DependentUpon>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ContentDotNet.Extensions.H26x\ContentDotNet.Extensions.H26x.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Containers\Container32.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>Container32.gen.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Containers\CabacCavlcContainer32.gen.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>CabacCavlcContainer32.tt</DependentUpon>
    </Compile>
    <Compile Update="Containers\Container32.gen.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Container32.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="ContentDotNet.Extensions.H264.Tests" />
    <InternalsVisibleTo Include="Dump264" />
  </ItemGroup>

</Project>
